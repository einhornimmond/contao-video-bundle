{
  "name": "heimrichhannot/contao-video-bundle",
  "type": "contao-bundle",
  "description": "This bundle brings enhanced video embedding support to contao with additional privacy options.",
  "keywords": [
    "contao",
    "youtube",
    "responsive",
    "video",
    "content"
  ],
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.1|^8.0",
    "contao/core-bundle": "^4.4",
    "symfony/http-kernel": "^3.4|^4.0|^5.4",
    "heimrichhannot/contao-utils-bundle": "^2.60"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\VideoBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\VideoBundle\\ContaoManager\\Plugin",
    "foxy": true
  },
  "suggest": {
    "heimrichhannot/contao-encore-bundle": "Manage dependencies with Symfony Webpack Encore."
  }
}
